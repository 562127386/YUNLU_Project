        //    });
        //}
        var parentId = top.$.jfinetab.getCurrentTabId();
        var Id = $("#gridList").jqGridRowValue().Id;
        if ($.checkedArray(Id)) {
            top.$.jfinetab.addTabContent("/TN_XM/TNRD_TransactionProtocol/Details?keyValue=" + Id, "转让协议--查看", parentId + "_view", parentId);
        }
    }
    function refreshGrid() {
        $("#gridList").trigger("reloadGrid");
    }
</script>

