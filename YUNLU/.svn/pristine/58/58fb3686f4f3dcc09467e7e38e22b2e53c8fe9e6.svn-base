<script>
    $(function () {
        gridList();
    })
    function gridList() {
        var $gridList = $("#gridList");
        $gridList.dataGrid({
            url: "/TN_XM/TNRD_TransactionProtocol/GetGridJson",
            height: $(window).height() - 138,
            colModel: [
                { label: "主键", name: "Id", hidden: true },
                {
                    label: "状态", name: "Status", width: 150, align: 'left', formatter: function (value, row, index) {
                        if (value == 0) {
                            return "新建";
                        }
                        else if (value == 1) {
                            return "已提交";
                        }
                        else if (value == 2) {
                            return "撤销"
                        }
                    }
                },

                { label: "转让单号", name: "TransactionNum", width: 150, align: 'left' },

                { label: "合同编码", name: "ContractCode", width: 150, align: 'left' },

                { label: "合同名称", name: "ContractName", width: 150, align: 'left' },

                { label: "项目编码", name: "ProjectCode", width: 150, align: 'left' },

                { label: "项目名称", name: "ProjectName", width: 150, align: 'left' },

                { label: "转让甲方", name: "TransactionA_Name", width: 150, align: 'left' },

                { label: "转让乙方", name: "TransactionB_Name", width: 150, align: 'left' },

                { label: "供应商", name: "Supplier", width: 150, align: 'left' },

                { label: "签订日期", name: "SignDate", width: 150, align: 'left' },

                { label: "签订人", name: "SignName", width: 150, align: 'left' },

                { label: "创建日期", name: "CreateDate", width: 150, align: 'left' },

                { label: "创建人", name: "CreateUserName", width: 150, align: 'left' },

                { label: "最后修改日期", name: "UpdateDate", width: 150, align: 'left' },

                { label: "最后修改人", name: "UpdateUserName", width: 150, align: 'left' },

            ],
            pager: "#gridPager",
            sortname: 'CreateDate desc',
            viewrecords: true
        });
        $("#btn_search").click(function () {
            var queryJson = {
                keyword: $("#txt_keyword").val()
            }
            $gridList.jqGrid('setGridParam', {
                postData: { queryJson: JSON.stringify(queryJson) }, page: 1
            }).trigger('reloadGrid');
        });
    }
    function btn_add() {
        //$.modalOpen({
        //    id: "Form",
        //    title: "新增转让协议",
        //    url: "/TN_XM/TNRD_TransactionProtocol/Form",
        //    width: ($(window).width() * 0.8) + "px",
