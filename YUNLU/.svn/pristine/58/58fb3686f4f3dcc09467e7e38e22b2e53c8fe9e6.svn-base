        //    }
        //});
        var parentId = top.$.jfinetab.getCurrentTabId();
        top.$.jfinetab.addTabContent("/TN_XM/TNRD_TransactionProtocol/Form", "转让协议--新增", parentId + "_add", parentId);
    }
    function btn_edit() {
        var keyValue = $("#gridList").jqGridRowValue().Id;
        //     if ($.checkedArray(keyValue))
        //     {
        //$.modalOpen({
        //	id: "Form",
        //	title: "修改转让协议",
        //	url: "/TN_XM/TNRD_TransactionProtocol/Form?keyValue=" + keyValue,
        //	width: ($(window).width() * 0.8) + "px",
        //	 height: ($(window).height() * 0.7) + "px",
        //	callBack: function (iframeId) {
        //		top.frames[iframeId].submitForm();
        //	}
        //});
        //     }
        var parentId = top.$.jfinetab.getCurrentTabId();
        var Id = $("#gridList").jqGridRowValue().Id;
        var Status = $("#gridList").jqGridRowValue().Status;
        if ($.checkedArray(Id)) {
            if (Status == "已提交") {
                $.modalAlert("已提交数据不可修改", "error");
                return;
            }
            top.$.jfinetab.addTabContent("/TN_XM/TNRD_TransactionProtocol/Form?keyValue=" + Id, "转让协议--修改", parentId + "_edit", parentId);
        }

    }
    function btn_delete() {
        var keyValue = $("#gridList").jqGridRowValue().Id;
        var Status = $("#gridList").jqGridRowValue().Status;
        if ($.checkedArray(keyValue)) {
            if (Status == "已提交") {
                $.modalAlert("已提交数据不可删除", "error");
                return;
            }
            $.deleteForm({
                url: "/TN_XM/TNRD_TransactionProtocol/DeleteForm",
                param: { keyValue: keyValue },
                success: function () {
                    $.currentWindow().$("#gridList").trigger("reloadGrid");
                }
            });
        }

    }
    function btn_details() {
        //var keyValue = $("#gridList").jqGridRowValue().Id;
        //if ($.checkedArray(keyValue)) {
        //    $.modalOpen({
