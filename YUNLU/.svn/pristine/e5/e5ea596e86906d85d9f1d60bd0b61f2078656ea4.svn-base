            url: "/YL_IT_FirstConfirm/YL_IT_FirstConfirm/SaveForm?keyValue=" + keyValue,
            param: postData,
            success: function () {
                $.currentWindow().$("#gridList").trigger("reloadGrid");
            }
        })
    }
</script>

