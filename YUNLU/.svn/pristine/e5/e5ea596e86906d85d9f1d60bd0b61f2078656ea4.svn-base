            $.ajax({
                url: "/YL_IT_FirstConfirm/YL_IT_FirstConfirm/GetFormJson",
                data: { keyValue: keyValue },
                dataType: "json",
                async: false,
                success: function (data) {
                    $("#form1").formSerialize(data);
                }
            });
        }
    });

	//初始化组件
    function initControl() {
        
    }

    function submitForm() {
